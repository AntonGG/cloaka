<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.sensey.privaton">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme" >
        <activity android:name="app.sensey.privaton.activity.SplashActivity" android:screenOrientation="userPortrait" android:theme="@style/Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="app.sensey.privaton.activity.TrashActivity" android:screenOrientation="userPortrait" android:theme="@style/Fullscreen"/>

        <activity android:name="app.sensey.privaton.activity.HelpActivity" android:screenOrientation="userPortrait"/>
        <activity android:name="app.sensey.privaton.activity.GridActivity" android:screenOrientation="userPortrait"/>
        <activity android:name="app.sensey.privaton.activity.Activity" android:screenOrientation="userPortrait"/>
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>
    </application>
</manifest>
